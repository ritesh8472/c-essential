#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int arr[20],n,temp,i,j=0,k;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int rev[n];
    for(i=n-1;i>=0;i--){
        rev[j]=arr[i];
        j++;
    }  
    for(k=0;k<n;k++){
        printf("%d ",rev[k]);
    }
    return 0;
}
