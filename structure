#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

struct car{
        int id;
        char model[20];
        int rent;
    };

int main() {
    
    struct car c[20];
    int i,n;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&c[i].id);
        scanf("%s",c[i].model);
        scanf("%d",&c[i].rent);
    }
    for(i=0;i<n;i++){
        printf("Car ID: %d\n",c[i].id);
        printf("Model: %s\n",c[i].model);
        printf("Rental Rate per Day: %d\n",c[i].rent);
        printf("Total Rental Cost for Car %d: %d\n",i+1,c[i].rent*10);
    }
    
    
    return 0;
}
